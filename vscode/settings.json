{
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true
  },
  "window.zoomLevel": 2,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#ffffff23",
    "editor.rangeHighlightBackground": "#ffffff23",
    "editor.wordHighlightStrongBackground": "#3e4998",
    "editor.wordHighlightBackground": "#3e4998",
    "editor.findMatchBackground": "#282c34",
    "editor.findMatchHighlightBackground": "#0b790e97",
    "editor.selectionForeground": "#99eeff"
  },
  "terminal.integrated.shell.osx": "/bin/zsh",
  "editor.tabSize": 2,
  "editor.scrollBeyondLastLine": false,
  "editor.minimap.enabled": false,
  "prettier.jsxBracketSameLine": true,
  "prettier.jsxSingleQuote": true,
  "prettier.semi": false,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 100,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.matchBrackets": "never",
  "workbench.editor.showTabs": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "jsx-sublime-babel-tags": "javascriptreact"
  },
  "diffEditor.renderSideBySide": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "scoper.highlightColor": "",
  "keyboard.touchbar.ignored": [
    "play",
    "workbench.action.debug.start",
    "workbench.action.debug.run"
  ],
  "workbench.activityBar.visible": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.cursorBlinking": "smooth",
  "editor.multiCursorMergeOverlapping": true,
  "terminal.integrated.rendererType": "dom",
  "workbench.startupEditor": "newUntitledFile",
  "openInGitHub.defaultBranch": "master",
  "openInGitHub.defaultRemote": "origin",
  "synthwave84.disableGlow": true,
  "workbench.colorCustomizations": {
    "[Night Owl]": {
      // "editor.background": "#000"
    }
  },
  "synthwave84.brightness": 0.1,
  "openInGithub.maxBuffer": 512000,
  "openInGitHub.providerType": "gitlab",
  "openInGitHub.excludeCurrentRevision": false,
  "files.exclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/*.js": { "when": "$(basename).ts" },
    // "**/node_modules/**": true,
    // "**/tmp/**": true,
    "**/.git": true,
    // "**/.svn": true,
    // "**/.hg": true,
    // "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "report*": true,
    // "**/log/**": true,
    // "**/.fdk/**": true
    "**/.cache/**": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/log/**": true,
    "**/logs/**": true,
    "**/.fdk/**": true,
    "**/public/**": true,
    "**/.cache/**": true
  },
  "search.exclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/log/**": true
  },
  "editor.formatOnSave": true,
  "git.autofetch": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "macros.list": {
    "leftOneWord": [{ "command": "type", "args": { "text": "b" } }],
    "rightOneWord": [{ "command": "type", "args": { "text": "e" } }],
    "moveSelectDown": [{ "command": "type", "args": { "text": "y']p']" } }],
    "moveSelectUp": [{ "command": "type", "args": { "text": "ykp" } }],
    "selectTilLineEnd": [{ "command": "type", "args": { "text": "v$h" } }],
    "selectPreviousChunk": [{ "command": "type", "args": { "text": "BBviW" } }],
    "selectNextChunk": [{ "command": "type", "args": { "text": "BBviW" } }],
    "selectAll": [{ "command": "type", "args": { "text": "ggVG" } }],
    "copySelectedToClipboard": [{ "command": "type", "args": { "text": "\"+y" } }],
    "moveLineDown": [{ "command": "type", "args": { "text": ":'<,'> move +2<Enter>" } }],
    "moveLineUp": [{ "command": "type", "args": { "text": ":'<,'> move -2<Enter>" } }],
    "iMoveDown": [
      { "command": "type", "args": { "text": "i" } },
      "editor.action.moveLinesDownAction"
    ],
    "fzyfind": [
      "editor.action.clipboardCopyAction",
      "fuzzySearch.activeTextEditor",
      "editor.action.clipboardPasteAction"
    ],
    "comment": [{ "command": "type", "args": { "text": "gcc" } }],
    "nextSearchedItem": [
      { "command": "editor.action.nextMatchFindAction" },
      { "command": "type", "args": { "text": "i<esc>" } }
    ],
    "openToTheSide": [{ "command": "editor.action.revealDefinition" }]
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "text.html.markdown.journal.task.open.bullet",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.open.marker",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.open.keyword",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.open.text",
        "settings": {}
      },
      {
        "scope": "text.html.markdown.journal.task.completed.keyword",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.completed.marker",
        "settings": {
          "foreground": "#AAAAAA"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.completed.text",
        "settings": {
          "foreground": "#AAAAAA"
        }
      },
      {
        "scope": "text.html.markdown.journal.task.completed.bullet",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "text.html.markdown.journal.memo.keyword",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "text.html.markdown.journal.memo.bullet",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "text.html.markdown.journal.scope",
        "settings": {
          "foreground": "#FFFF00"
        }
      },
      {
        "scope": "text.html.markdown.journal.link.keyword",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "text.html.markdown.journal.link.bullet",
        "settings": {
          "foreground": "#FFFF00"
        }
      }
    ]
  },
  "search.searchOnType": false,
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "workbench.colorTheme": "Night Owl"
}
