{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "One Monokai",
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#ffffff23",
        "editor.rangeHighlightBackground": "#ffffff23",
        "editor.wordHighlightStrongBackground": "#3e4998",
        "editor.wordHighlightBackground": "#3e4998",
        // "editor.hoverHighlightBackground": "#00ff00",
        "editor.findMatchBackground": "#282c34",
        "editor.findMatchBorder": "#ff0000",
        "editor.findMatchHighlightBackground": "#0b790e97",
        "editor.selectionForeground": "#99eeff",
    },
    "terminal.integrated.shell.osx": "/bin/zsh",
    "window.zoomLevel": 1,
    "editor.tabSize": 2,
    "editor.quickSuggestions": false,
    "editor.parameterHints.enabled": false,
    "workbench.view.experimental.allowMovingToNewContainer": true,
    "multiCommand.commands": [
        {
            "command": "multiCommand.openInEditor",
            "sequence": [
                "explorer.openToSide",
                "workbench.action.moveEditorToPreviousGroup"
            ]
        },
    ],
    // "workbench.editor.enablePreview": false,
    // "files.associations": {
    //     "*.tsx": "typescriptreact",
    //     "*.jsx": "javascriptreact"
    // },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.minimap.enabled": false,
    "prettier.jsxBracketSameLine": true,
    "prettier.jsxSingleQuote": true,
    "prettier.semi": false,
    "prettier.tabWidth": 2,
    "prettier.printWidth": 100,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "workbench.editor.showTabs": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "typescript": "typescriptreact",
        "jsx-sublime-babel-tags": "javascriptreact"
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/tmp": true,
        "**/coverage": true,
        "**/ios": true,
        "**/android": true,
        "_build": true,
        "deps": true
    },
    "editor.formatOnSave": true,
    "git.autofetch": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "sync.autoDownload": true,
    "sync.autoUpload": true,
    "macros.list": {
        "selectInnerDblQuotes": [
            {"command": "type", "args": {"text": "s"}},
            {"command": "type", "args": {"text": "i"}},
            {"command": "type", "args": {"text": "\""}},
        ],
        "deleter": [
            {"command": "settings.cycle.delete",},
            {"command": "type", "args": {"text": "d"}}
        ]

    },
    "settings.cycle": [{
        "id": "delete",
        "overrideWorkspaceSettings": true,
        "values": [{
            "titleBar.activeBackground": "#ff0000"
        }
        ]
    }
],
"editor.tokenColorCustomizations": {
    "textMateRules": [
        {
            "scope": "text.html.markdown.journal.task.open.bullet",
            "settings": {
                "foreground": "#FFFF00"
            }
        },
        {
            "scope": "text.html.markdown.journal.task.open.marker",
            "settings": {
                "foreground": "#FFFF00"
            }
        },
        {
            "scope": "text.html.markdown.journal.task.open.keyword",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "text.html.markdown.journal.task.open.text",
            "settings": {}
        },
        {
            "scope": "text.html.markdown.journal.task.completed.keyword",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "text.html.markdown.journal.task.completed.marker",
            "settings": {
                "foreground": "#AAAAAA"
            }
        },
        {
            "scope": "text.html.markdown.journal.task.completed.text",
            "settings": {
                "foreground": "#AAAAAA"
            }
        },
        {
            "scope": "text.html.markdown.journal.task.completed.bullet",
            "settings": {
                "foreground": "#FFFF00"
            }
        },
        {
            "scope": "text.html.markdown.journal.memo.keyword",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "text.html.markdown.journal.memo.bullet",
            "settings": {
                "foreground": "#FFFF00"
            }
        },
        {
            "scope": "text.html.markdown.journal.scope",
            "settings": {
                "foreground": "#FFFF00"
            }
        },
        {
            "scope": "text.html.markdown.journal.link.keyword",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "text.html.markdown.journal.link.bullet",
            "settings": {
                "foreground": "#FFFF00"
            }
        }
    ]
},
"diffEditor.renderSideBySide": false
}
